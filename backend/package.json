{
  "name": "food-app-backend-ai-powered",
  "version": "2.0.0",
  "description": "AI-Powered Food App Backend with Advanced Features",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "ai:recommendations": "python3 ai/ai_recommendation_engine.py",
    "ai:voice": "python3 ai/voice_processor.py",
    "ai:route": "python3 ai/route_optimizer.py",
    "test": "jest",
    "build": "npm run ai:setup",
    "ai:setup": "python3 -m pip install -r ai/requirements.txt"
  },
  "dependencies": {
    "express": "^4.18.2",
    "razorpay": "^2.9.2",
    "stripe": "^19.3.0",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express-rate-limit": "^6.10.0",
    "ws": "^8.14.2",
    "uuid": "^9.0.1",
    "node-cron": "^3.0.3",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "mongoose": "^8.0.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.52.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "food-delivery",
    "ai",
    "machine-learning",
    "voice-processing",
    "ar-vr",
    "real-time-chat",
    "route-optimization",
    "dynamic-pricing",
    "sustainability",
    "loyalty"
